tw:ConferencePaper a rdfs:Class,
        rdfs:Resource,
        owl:Class ;
    rdfs:label "Conference Paper"@en ;
    rdfs:comment "A paper published in the proceedings of a conference"@en ;
    rdfs:subClassOf tw:ProceedingPaper .

tw:ProceedingPaper a rdfs:Class,
        rdfs:Resource,
        owl:Class ;
    rdfs:label "Proceeding Paper"@en ;
    rdfs:comment "A paper published in a conference proceedings"@en ;
    rdfs:subClassOf tw:Publication .

tw:Publication a rdfs:Class,
        rdfs:Resource,
        owl:Class ;
    rdfs:label "Publication"@en ;
    rdfs:comment "A formal publication of a work, usually research related (although not necessarily)"@en ;
    rdfs:subClassOf foaf:Document .

